{
  "name_rules": [
    { "old": "database", "new": "records" },
    { "old": "document", "new": "item" }
  ],
  "insert_rules": [
    {
      "target": "status",
      "template": "<insertBefore>%d</insertBefore>",
      "counter": "insert_counter"
    }
  ],
  "wrap_rules": [
    {
      "target": "item",
      "wrapper": "content"
    }
  ],
  "value_rules": [
    {
      "target": "id",
      "type": "prepend",
      "params": {
        "prefix": "ID-"
      }
    }
  ],
  "counters": {
    "insert_counter": {
      "start": 0
    }
  }
}